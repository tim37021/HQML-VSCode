{
	"name": "hqml",
	"publisher": "HcMusic",
	"repository": "https://github.com/tim37021/HQML-VSCode",
	"displayName": "HQML",
	"description": "HQML language support and tools",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.create",
		"onCommand:extension.build",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.create",
				"title": "Create HQML project"
			},
			{
				"command": "extension.build",
				"title": "Build HQML project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/unzip": "^0.1.1",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"unzip": "^0.1.11"
	}
}
